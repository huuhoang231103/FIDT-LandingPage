# Frontend Environment Configuration
# Copy this file to .env and update with your actual values

# API Configuration
VITE_API_BASE_URL=https://thinhvuongtaichinh.net/backend
VITE_APP_NAME=FIDT Landing Page
VITE_APP_VERSION=1.0.0

# Email Configuration (Example - Update with your actual values)
VITE_CONTACT_EMAIL=your-email@domain.com
VITE_REPLY_TO_EMAIL=noreply@yourdomain.com

# Example configurations:
# VITE_CONTACT_EMAIL=admin@fidt.com
# VITE_REPLY_TO_EMAIL=contact@fidt.com
# VITE_CONTACT_EMAIL=info@fidt.com

# Domain Configuration (Example - Update with your actual domain)
VITE_DOMAIN=yourdomain.com
VITE_SITE_URL=https://yourdomain.com

# Example configurations:
# VITE_DOMAIN=fidt.com
# VITE_SITE_URL=https://fidt.com
# VITE_DOMAIN=example.com
# VITE_SITE_URL=https://example.com

# Development Configuration
VITE_DEV_MODE=true
VITE_DEBUG_LEVEL=info

# Production Configuration (Update when deploying)
# VITE_DEV_MODE=false
# VITE_DEBUG_LEVEL=error
# VITE_API_BASE_URL=https://yourdomain.com/backend
# VITE_SITE_URL=https://yourdomain.com

# Analytics Configuration (Optional)
VITE_GA_TRACKING_ID=GA-XXXXXXXXX
VITE_FACEBOOK_PIXEL_ID=XXXXXXXXXX

# Social Media Configuration (Optional)
VITE_FACEBOOK_URL=https://facebook.com/yourpage
VITE_TWITTER_URL=https://twitter.com/yourhandle
VITE_LINKEDIN_URL=https://linkedin.com/company/yourcompany
VITE_INSTAGRAM_URL=https://instagram.com/yourprofile

# Contact Information (Optional)
VITE_PHONE_NUMBER=+84 123 456 789
VITE_ADDRESS=Your Address, City, Country
VITE_BUSINESS_HOURS=Monday - Friday: 9:00 AM - 6:00 PM

# Feature Flags (Optional)
VITE_ENABLE_CHAT=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SOCIAL_LOGIN=false
